<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
        <item android:state_enabled="true"
            android:drawable="@drawable/icon_feather_star_borderblack" />
        <item android:state_selected="true"
            android:drawable="@drawable/icon_awesome_star_yello" />
        <item android:state_pressed="true"
            android:drawable="@drawable/icon_awesome_star_yello" /> <!-- pressed -->
        <item android:state_focused="true"
            android:drawable="@drawable/icon_awesome_star_yello" /> <!-- focused -->
        <item android:drawable="@drawable/icon_feather_star_borderblack" /> <!-- default -->
        <item android:state_pressed="true"
            android:drawable="@android:drawable/btn_star_big_on" /> <!-- pressed -->
        <item android:state_focused="true"
            android:drawable="@android:drawable/btn_star_big_on" /> <!-- focused -->
        <item android:state_hovered="true"
            android:drawable="@android:drawable/btn_star_big_off" /> <!-- hovered -->
        <item android:drawable="@android:drawable/btn_star" /> <!-- default -->

</selector>